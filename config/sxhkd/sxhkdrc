super + Return
    alacritty

super + d
	dmenu_run

super + ctrl  + l
	~/bin/lock.sh

super + p
    ~/bin/pwmenu.sh

XF86AudioRaiseVolume
    pamixer -i 2

XF86AudioLowerVolume
    pamixer -d 2

XF86AudioMute
    pamixer -t

XF86MonBrightnessUp
   xbacklight -inc 5

XF86MonBrightnessDown 
   xbacklight -dec 5

Print
   scr -f

ctrl + Print 
   scr -w

ctrl + shift + Print
   scr -s
