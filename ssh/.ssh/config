Host *
	ServerAliveInterval 20
	ServerAliveCountMax 2

Host github.com
  IdentityFile ~/.ssh/github
  Hostname ssh.github.com
  Port 443

Host gitlab.com
  IdentityFile ~/.ssh/github
  Hostname altssh.gitlab.com
  User git
  PreferredAuthentications publickey
  Port 443	
 
Host droplet
  IdentityFile ~/.ssh/id_rsa
  Hostname 139.59.95.34
  User root
  Port 443  

